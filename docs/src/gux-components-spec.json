{
  "gux-accordion": {
    "attributes": [{ "name": "heading-level", "type": "text" }]
  },
  "gux-button": {
    "attributes": [
      { "name": "disabled", "type": "checkbox" },
      { "name": "title", "type": "text" },
      { "name": "text", "type": "text" },
      { "name": "left-icon", "type": "text" },
      { "name": "right-icon", "type": "text" },
      {
        "name": "accent",
        "type": "select",
        "options": ["primary", "secondary"]
      }
    ],
    "events": [
      { "name": "click", "description": "Triggered when element is clicked" }
    ]
  },
  "gux-action-button": {
    "attributes": [
      { "name": "disabled", "type": "checkbox" },
      { "name": "title", "type": "text" },
      { "name": "text", "type": "text" },
      { "name": "is-open", "type": "checkbox" },
      {
        "name": "accent",
        "type": "select",
        "options": ["primary", "secondary"]
      }
    ],
    "events": [
      { "name": "open", "description": "Triggered when element is opened" },
      { "name": "close", "description": "Triggered when element is closed" },
      {
        "name": "actionClick",
        "description": "Triggered when element is clicked"
      }
    ]
  },
  "gux-action-toast": {
    "attributes": [
      { "name": "toast-title", "type": "text" },
      { "name": "message", "type": "text" },
      { "name": "subject", "type": "text" },
      { "name": "icon", "type": "text" },
      { "name": "right-button", "type": "json" },
      { "name": "left-button", "type": "json" },
      { "name": "key-values", "type": "json" }
    ]
  },
  "gux-advanced-dropdown": {
    "attributes": [{ "name": "disabled", "type": "checkbox" }],
    "events": [
      { "name": "input", "description": "Item from dropdown selected" },
      { "name": "filter", "description": "Filter value changed" }
    ]
  },
  "gux-checkbox": {
    "attributes": [
      { "name": "checked", "type": "checkbox" },
      { "name": "indeterminate", "type": "checkbox" },
      { "name": "disabled", "type": "checkbox" }
    ],
    "events": [
      {
        "name": "check",
        "description": "Triggered when element's checked state is changed"
      }
    ]
  },
  "gux-color-picker": {
    "attributes": [
      { "name": "value", "type": "text" },
      { "name": "disabled", "type": "checkbox" },
      { "name": "custom-colors", "type": "json" }
    ],
    "events": [
      {
        "name": "input",
        "description": "Triggered when elements value is changed"
      }
    ]
  },
  "gux-color-select": {
    "attributes": [
      { "name": "value", "type": "text" },
      { "name": "custom-colors", "type": "json" }
    ],
    "events": [
      { "name": "input", "description": "Triggered when a color is clicked" }
    ]
  },
  "gux-disclosure-button": {
    "attributes": [
      { "name": "position", "type": "select", "options": ["left", "right"] },
      { "name": "label", "type": "text" }
    ],
    "events": [
      {
        "name": "active",
        "description": "Triggered when panel state is changed"
      }
    ]
  },
  "gux-dropdown": {
    "attributes": [
      {
        "name": "mode",
        "type": "select",
        "options": ["default", "page", "palette"]
      },
      { "name": "disabled", "type": "checkbox" },
      { "name": "filterable", "type": "checkbox" },
      { "name": "value", "type": "text" },
      { "name": "placeholder", "type": "text" },
      { "name": "items", "type": "json" }
    ],
    "events": [
      {
        "name": "change",
        "description": "Triggered when item is selected from dropdown"
      }
    ]
  },
  "gux-list": {
    "attributes": [{ "name": "items", "type": "json" }]
  },
  "gux-modal": {
    "attributes": [
      {
        "name": "size",
        "type": "select",
        "options": ["small", "medium", "large"]
      },
      { "name": "modal-title", "type": "text" }
    ],
    "events": [
      {
        "name": "close",
        "description": "Modal is signaling that it should be hidden"
      }
    ]
  },
  "gux-notification-toast": {
    "attributes": [
      { "name": "toast-title", "type": "text" },
      { "name": "icon", "type": "text" },
      {
        "name": "accent",
        "type": "select",
        "options": ["neutral", "positive", "alert", "warning"]
      },
      { "name": "message", "type": "text" }
    ],
    "events": [
      {
        "name": "closeClick",
        "description": "Triggered when the close icon is clicked"
      }
    ]
  },
  "gux-page-loading-spinner": {},
  "gux-panel-frame": {},
  "gux-radial-progress": {
    "attributes": [
      { "name": "value", "type": "text" },
      { "name": "max", "type": "text" }
    ]
  },
  "gux-icon": {
    "attributes": [
      { "name": "icon-name", "type": "text" },
      { "name": "decorative", "type": "checkbox" },
      { "name": "screenreader-text", "type": "text" }
    ]
  },
  "gux-radio": {
    "attributes": [
      { "name": "value", "type": "text" },
      { "name": "disabled", "type": "checkbox" }
    ],
    "events": [
      { "name": "check", "description": "Radio button checked state changed" }
    ]
  },
  "gux-rating": {
    "attributes": [
      { "name": "disabled", "type": "checkbox" },
      { "name": "rating", "type": "text" },
      { "name": "max-rating", "type": "text" },
      { "name": "allow-half-ratings", "type": "checkbox" }
    ],
    "events": [{ "name": "update", "description": "rating value changed" }]
  },
  "gux-slider": {
    "attributes": [
      { "name": "min", "type": "text" },
      { "name": "max", "type": "text" },
      { "name": "step", "type": "text" },
      { "name": "value", "type": "text" },
      { "name": "is-percentage", "type": "checkbox" },
      { "name": "display-text-box", "type": "checkbox" }
    ],
    "events": [{ "name": "update", "description": "slider value changed" }]
  },
  "gux-switch": {
    "attributes": [{ "name": "selected-value", "type": "text" }],
    "events": [
      { "name": "selectionChanged", "description": "switch state was changed" }
    ]
  },
  "gux-toggle": {
    "attributes": [
      { "name": "checked", "type": "checkbox" },
      { "name": "disabled", "type": "checkbox" },
      { "name": "checked-label", "type": "select", "options": ["on", "off"] },
      { "name": "unchecked-label", "type": "select", "options": ["on", "off"] }
    ],
    "events": [{ "name": "check", "description": "toggle state changed" }]
  },
  "gux-tooltip": {
    "attributes": [
      { "name": "text", "type": "text" },
      { "name": "for", "type": "text" },
      { "name": "delay", "type": "text" },
      { "name": "is-shown", "type": "checkbox" }
    ],
    "events": [
      {
        "name": "hidden",
        "description": "fired when tooltip moves to hidden state"
      },
      {
        "name": "shown",
        "description": "fired when tooltip moves to visible state"
      }
    ]
  },

  "gux-list-item": {
    "hidePage": true,
    "attributes": [{ "name": "disabled", "type": "checkbox" }],
    "events": [
      { "name": "press", "description": "list item selected within list" }
    ]
  },
  "gux-radio-group": {
    "hidePage": true,
    "attributes": [{ "name": "name", "type": "text" }],
    "events": [
      {
        "name": "input",
        "description": "Radio selection state is changed inside the group"
      }
    ]
  }
}
