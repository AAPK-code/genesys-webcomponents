{
  "name": "genesys-webcomponent",
  "version": "0.0.1",
  "description": "Common webcomponents",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run docs && npm run stencil build && npm run storybook",
    "stencil": "stencil build --docs",
    "stencil.dev": "stencil build --docs --dev --watch --serve --no-open",
    "test.unit": "stencil test --spec",
    "test.e2e": "stencil test --e2e",
    "test.screenshot": "stencil test --e2e --screenshot",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "storybook": "./node_modules/.bin/build-storybook -c .storybook -o .out",
    "storybook.dev": "./node_modules/.bin/start-storybook -p 9001 -c .storybook -s ./www,./src/style",
    "create-component": "node scripts/create-component.js && npm run docs",
    "dev": "./node_modules/.bin/concurrently  \"npm run storybook.dev\" \"npm run stencil.dev\"",
    "format": "prettier ./src/**/*.ts ./src/**/*.tsx",
    "format.fix": "npm run format -- --write",
    "lint": "tslint --project tsconfig.json",
    "lint.fix": "npm run lint -- --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "~0.13.2",
    "@stencil/less": "^0.1.0",
    "@stencil/utils": "0.0.5",
    "@stencil/webpack": "0.0.6",
    "@storybook/addon-actions": "4.0.0-alpha.21",
    "@storybook/addon-backgrounds": "4.0.0-alpha.21",
    "@storybook/addon-knobs": "4.0.0-alpha.21",
    "@storybook/addon-links": "4.0.0-alpha.21",
    "@storybook/addon-notes": "4.0.0-alpha.21",
    "@storybook/addon-options": "4.0.0-alpha.21",
    "@storybook/addon-storysource": "4.0.0-alpha.21",
    "@storybook/addon-viewport": "4.0.0-alpha.21",
    "@storybook/addons": "4.0.0-alpha.21",
    "@storybook/cli": "^3.4.10",
    "@storybook/html": "^4.0.0-alpha.11",
    "@storybook/polymer": "4.0.0-alpha.21",
    "@types/jest": "23.3.5",
    "@types/puppeteer": "1.6.0",
    "concurrently": "^4.0.1",
    "handlebars": "^4.0.11",
    "html-loader": "^0.5.5",
    "inquirer": "^6.2.0",
    "jest": "23.5.0",
    "jest-environment-node": "23.4.0",
    "lit-html": "^0.10.2",
    "markdown-loader": "^4.0.0",
    "moment": "^2.22.2",
    "pixelmatch": "4.0.2",
    "polymer-webpack-loader": "^2.0.3",
    "prettier": "^1.14.3",
    "puppeteer": "1.7.0",
    "storybook-readme": "^4.0.0-beta1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.13.0",
    "webpack": "^4.17.1",
    "webpack-dev-server": "^3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+bitbucket.org:inindca/genesys-webcomponents.git"
  }
}
