{
  "name": "genesys-webcomponents",
  "version": "0.0.1",
  "description": "Common webcomponents",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run i18n && npm run stencil && npm run storybook",
    "stencil": "stencil build --docs --prod",
    "stencil.dev": "stencil build --dev --watch --es5 --docs",
    "test.unit": "stencil test --spec",
    "test.e2e": "stencil test --e2e",
    "test.screenshot": "stencil test --e2e --screenshot",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "storybook": "./node_modules/.bin/build-storybook -c .storybook -o .out",
    "storybook.dev": "./node_modules/.bin/start-storybook -p 9001 -c .storybook -s ./www,./src/style,./dist",
    "create-component": "node scripts/create-component.js",
    "dev": "./node_modules/.bin/concurrently  \"npm run stencil.dev\" \"npm run storybook.dev\"",
    "format": "prettier ./src/**/*.ts ./src/**/*.tsx",
    "format.fix": "npm run format -- --write",
    "lint": "tslint --project .",
    "lint.fix": "npm run lint -- --fix",
    "i18n": "node scripts/build-i18n.js"
  },
  "dependencies": {
    "intl-messageformat": "^2.2.0"
  },
  "devDependencies": {
    "@stencil/core": "^0.15.2",
    "@stencil/less": "^0.1.0",
    "@stencil/utils": "0.0.5",
    "@stencil/webpack": "0.0.6",
    "@storybook/addon-a11y": "^4.0.4",
    "@storybook/addon-actions": "4.0.0-alpha.21",
    "@storybook/addon-backgrounds": "4.0.0-alpha.21",
    "@storybook/addon-knobs": "4.0.0-alpha.21",
    "@storybook/addon-links": "4.0.0-alpha.21",
    "@storybook/addon-notes": "4.0.0-alpha.21",
    "@storybook/addon-options": "4.0.0-alpha.21",
    "@storybook/addon-storysource": "4.0.0-alpha.21",
    "@storybook/addon-viewport": "4.0.0-alpha.21",
    "@storybook/addons": "4.0.0-alpha.21",
    "@storybook/cli": "^3.4.10",
    "@storybook/html": "^4.0.0-alpha.11",
    "@storybook/polymer": "4.0.0-alpha.21",
    "@types/jest": "23.3.5",
    "@types/puppeteer": "1.6.4",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "concurrently": "^4.0.1",
    "glob": "^7.1.3",
    "handlebars": "^4.0.11",
    "html-loader": "^0.5.5",
    "inquirer": "^6.2.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-environment-node": "23.4.0",
    "jest-junit": "^5.2.0",
    "lit-html": "^0.10.2",
    "markdown-loader": "^4.0.0",
    "moment": "^2.22.2",
    "pixelmatch": "4.0.2",
    "polymer-webpack-loader": "^2.0.3",
    "prettier": "^1.14.3",
    "puppeteer": "1.8.0",
    "rollup-plugin-copy-glob": "^0.2.2",
    "storybook-readme": "^4.0.2",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.13.0",
    "webpack": "^4.26.1",
    "webpack-dev-server": "^3.1.6",
    "workbox-build": "3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+bitbucket.org:inindca/genesys-webcomponents.git"
  }
}
